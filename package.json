{
  "name": "gauge-js",
  "version": "2.0.0",
  "description": "JavaScript runner for Gauge",
  "main": "index.js",
  "scripts": {
    "check-style": "./node_modules/.bin/jscs *.js src/ skel/ test/",
    "lint": "./node_modules/.bin/jshint *.js src/ skel/ test/ scripts/",
    "test": "npm run check-style && npm run lint && ./node_modules/.bin/mocha --recursive",
    "installPlugin": "node ./scripts/install.js",
    "package": "node ./scripts/install.js --package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/getgauge-contrib/gauge-js.git"
  },
  "keywords": [
    "gauge",
    "JavaScript"
  ],
  "author": "Renjith G R",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/getgauge-contrib/gauge-js/issues"
  },
  "homepage": "https://github.com/getgauge-contrib/gauge-js",
  "dependencies": {
    "escodegen": "^1.8.0",
    "esprima": "^2.7.2",
    "estraverse": "^4.1.1",
    "protobufjs": "~6.8.0",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "archiver": "^0.21.0",
    "chai": "^3.4.0",
    "fs-extra": "^0.26.4",
    "jscs": "^2.8.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "sinon": "^1.17.2"
  }
}
