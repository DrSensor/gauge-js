  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/renjithgr/gauge-js/issues"
  },
  "homepage": "https://github.com/renjithgr/gauge-js",
  "dependencies": {
    "protobufjs": "^4.1.2",
    "protocol-buffers": "^3.1.3",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "fs-extra": "^0.26.4",
    "jscs": "^2.8.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "sinon": "^1.17.2"
  }
